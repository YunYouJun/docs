    "@iconify-json/vscode-icons": "catalog:",
    "@shikijs/types": "catalog:",
    "@types/node": "catalog:",
    "@unocss/eslint-plugin": "catalog:",
    "bumpp": "catalog:",
    "conventional-changelog-cli": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-format": "catalog:",
    "vue-tsc": "catalog:"
  }
}
