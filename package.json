{
  "name": "@yunyoujun/docs",
  "type": "module",
  "version": "0.1.13",
  "private": true,
  "packageManager": "pnpm@10.26.2",
  "description": "Docs for YunYouJun Projects.",
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://github.com/YunYouJun"
  },
  "license": "ISC",
  "keywords": [
    "docs",
    "yunyoujun"
  ],
  "main": "index.js",
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "dev": "pnpm run docs:dev",
    "build": "pnpm run docs:build",
    "preview": "pnpm run docs:preview",
    "docs:dev": "pnpm -C docs run docs:dev",
    "docs:build": "pnpm -C docs run build",
    "docs:preview": "pnpm -C docs run docs:preview",
    "lint": "eslint .",
    "release": "bumpp",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@iconify-json/logos": "catalog:",
    "@iconify-json/ri": "catalog:",
