    "@iconify-json/vscode-icons": "^1.2.23",
    "@shikijs/types": "catalog:",
    "@types/node": "^24.0.10",
    "@unocss/eslint-plugin": "^66.3.3",
    "bumpp": "^10.2.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-format": "^1.0.1",
    "vue-tsc": "^3.0.1"
