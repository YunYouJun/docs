{
  "name": "@yunyoujun/docs",
  "type": "module",
  "version": "0.0.8",
  "private": true,
  "packageManager": "pnpm@10.6.5",
  "description": "Docs for YunYouJun Projects.",
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://github.com/YunYouJun"
  },
  "license": "ISC",
  "keywords": [
    "docs",
    "yunyoujun"
  ],
  "main": "index.js",
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "dev": "pnpm run docs:dev",
    "build": "pnpm run docs:build",
    "preview": "pnpm run docs:preview",
    "docs:dev": "pnpm -C docs run docs:dev",
    "docs:build": "pnpm -C docs run docs:build",
    "docs:preview": "pnpm -C docs run docs:preview",
    "lint": "eslint .",
    "release": "bumpp --all --execute=\"npm run changelog\"",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.1",
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/ri": "^1.2.5",
    "@iconify-json/simple-icons": "^1.2.29",
    "@iconify-json/vscode-icons": "^1.2.16",
    "@shikijs/types": "catalog:",
    "@types/node": "^22.13.10",
    "@unocss/eslint-plugin": "^66.0.0",
    "bumpp": "^10.1.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.22.0",
    "eslint-plugin-format": "^1.0.1",
    "vue-tsc": "^2.2.8"
  }
}
