{
  "name": "@yunyoujun/docs",
  "type": "module",
  "version": "0.1.12",
  "private": true,
  "packageManager": "pnpm@10.12.4",
  "description": "Docs for YunYouJun Projects.",
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://github.com/YunYouJun"
  },
  "license": "ISC",
  "keywords": [
    "docs",
    "yunyoujun"
  ],
  "main": "index.js",
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "dev": "pnpm run docs:dev",
    "build": "pnpm run docs:build",
    "preview": "pnpm run docs:preview",
    "docs:dev": "pnpm -C docs run docs:dev",
    "docs:build": "pnpm -C docs run build",
    "docs:preview": "pnpm -C docs run docs:preview",
    "lint": "eslint .",
    "release": "bumpp",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@iconify-json/logos": "^1.2.4",
