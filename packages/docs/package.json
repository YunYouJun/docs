{
  "name": "@yunyoujun/docs",
  "type": "module",
  "version": "0.1.13",
  "description": "Some components/utils for docs.",
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "homepage": "https://docs.yunyoujun.cn",
  "repository": {
    "type": "git",
    "url": "https://github.com/YunYouJun/docs"
  },
  "keywords": [
    "docs",
    "vitepress",
    "config",
    "vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.mjs"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs"
    },
    "./types": {
      "import": "./dist/types/index.d.ts"
    },
    "./styles": "./client/styles/index.ts",
    "./*": "./*"
  },
  "main": "./dist/node/index.mjs",
  "files": [
    "bin",
    "client",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --no-tag --no-commit --no-push && pnpm publish --no-git-checks"
  },
  "dependencies": {
    "dayjs": "^1.11.18",
    "markdown-it-task-lists": "^2.1.1",
    "prompts": "^2.4.2",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@shikijs/types": "catalog:",
    "@shikijs/vitepress-twoslash": "catalog:",
    "shiki": "catalog:",
    "unbuild": "^3.6.1",
    "unocss": "^66.5.2",
    "unplugin-vue-components": "^29.1.0",
    "vite": "catalog:",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitepress": "catalog:",
    "vitepress-plugin-group-icons": "^1.6.3"
  }
}
